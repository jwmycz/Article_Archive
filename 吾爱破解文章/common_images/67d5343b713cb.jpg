<!-- 微信二维码弹窗 -->
<div class="sidebar-modal" id="wechat-modal">
    <div class="modal-content">
        <h3><i class="fab fa-weixin"></i> 添加微信</h3>
        <img class="qrcode-img" src="https://download.nature.qq.com/SnsShare/lengxi/1765553382_0587367d_6c19fc004df61d38173bd9160563f43.jpg" alt="微信二维码">
        <p>扫描二维码添加微信</p>
        <button class="close-btn" id="close-wechat">关闭</button>
    </div>
</div>

<script>
    // 辅助函数：显示弹窗
    function showModal(modalId) {
        const modal = document.getElementById(modalId);
        modal.style.display = 'flex';
        // 触发重绘后添加active类以启动动画
        setTimeout(() => {
            modal.classList.add('active');
        }, 10);
    }
    
    // 辅助函数：隐藏弹窗
    function hideModal(modalId) {
        const modal = document.getElementById(modalId);
        modal.classList.remove('active');
        // 动画结束后隐藏
        setTimeout(() => {
            modal.style.display = 'none';
        }, 300);
    }

    // 联系我们
    document.getElementById('sb-contact').addEventListener('click', function() {
        showModal('contact-modal');
    });
    
    // 微信二维码
    document.getElementById('sb-wechat').addEventListener('click', function() {
        showModal('wechat-modal');
    });
    
    // QQ群跳转
    document.getElementById('sb-qq').addEventListener('click', function() {
        window.open('https://qm.qq.com/cgi-bin/qm/qr?k=d9dFFV7n57yVHNx7Ne3iWwIdRKRwNls_&jump_from=webapi&authKey=YCIfKLmSWWtDZmQXx9PXBZZGD/vHEvLOVR0yov3IfVNa5ulNwnnuponT0LqULOQh', '_blank');
    });
    
    // 分享功能 - 仅保留复制链接功能
    document.getElementById('sb-share').addEventListener('click', function() {
        const shareItem = this;
        const originalIcon = shareItem.querySelector('i').className;
        const originalText = shareItem.querySelector('span').textContent;
        
        // 执行复制链接操作
        const input = document.createElement('input');
        input.value = window.location.href;
        document.body.appendChild(input);
        input.select();
        document.execCommand('copy');
        document.body.removeChild(input);
        
        // 显示复制成功提示
        shareItem.querySelector('i').className = 'fas fa-check';
        shareItem.querySelector('span').textContent = '已复制链接';
        shareItem.style.color = '#52c41a'; // 成功绿色
        
        // 2秒后恢复原始状态
        setTimeout(() => {
            shareItem.querySelector('i').className = originalIcon;
            shareItem.querySelector('span').textContent = originalText;
            shareItem.style.color = ''; // 恢复默认颜色
        }, 2000);
    });
    
    // 关闭按钮
    document.getElementById('close-contact').addEventListener('click', function() {
        hideModal('contact-modal');
    });
    
    document.getElementById('close-wechat').addEventListener('click', function() {
        hideModal('wechat-modal');
    });
    
    // 点击外部关闭
    window.addEventListener('click', function(e) {
        if (e.target.classList.contains('sidebar-modal')) {
            hideModal(e.target.id);
        }
    });
    
    // 按ESC键关闭弹窗
    document.addEventListener('keydown', function(e) {
        if (e.key === 'Escape') {
            const activeModals = document.querySelectorAll('.sidebar-modal.active');
            activeModals.forEach(modal => {
                hideModal(modal.id);
            });
        }
    });
</script>






