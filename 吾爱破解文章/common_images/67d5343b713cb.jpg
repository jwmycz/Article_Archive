<script>
// 关闭弹窗功能
const modal = document.getElementById('bonusModal');
const closeBtn = document.getElementById('closeBonus');
const foreverCloseCheck = document.getElementById('foreverCloseCheck');

// 新增：判断是否为移动端（屏幕宽度≤768px）
function isMobile() {
  return window.innerWidth <= 768 || /Android|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);
}

// 设置永久关闭Cookie
function setForeverCloseCookie() {
  // 设置有效期为1年（365天），可根据需求调整
  document.cookie = "bonusForeverClose=1; max-age=31536000; path=/; SameSite=Lax";
}

// 读取Cookie
function getCookie(name) {
  const cookies = document.cookie.split('; ');
  for (let cookie of cookies) {
    const [cookieName, cookieValue] = cookie.split('=');
    if (cookieName === name) return cookieValue;
  }
  return null;
}

// 关闭弹窗（增加动画效果）
function closeModal() {
  // 添加淡出动画
  modal.style.opacity = '0';
  modal.style.transition = 'opacity 0.3s ease';
  
  // 动画结束后隐藏
  setTimeout(() => {
    modal.style.display = 'none';
  }, 300);
  
  // 如果勾选了永久关闭，则设置Cookie
  if (foreverCloseCheck.checked) {
    setForeverCloseCookie();
  }
}

// 绑定关闭按钮事件
closeBtn.onclick = closeModal;

// 点击遮罩层关闭
modal.onclick = function(e) {
  if (e.target === modal) closeModal();
};

// ESC键关闭
document.addEventListener('keydown', function(e) {
  // 移动端不响应ESC键
  if (!isMobile() && e.key === 'Escape' && modal.style.display !== 'none') {
    closeModal();
  }
});

// 阻止卡片内部点击冒泡到遮罩层
document.getElementById('bonusCard').addEventListener('click', function(e) {
  e.stopPropagation();
});

// 页面加载时检查：移动端直接隐藏，PC端按原有逻辑
window.addEventListener('load', function() {
  // 核心：移动端直接隐藏弹窗
  if (isMobile()) {
    modal.style.display = 'none';
    modal.style.opacity = '0';
    return; // 移动端直接退出，不执行后续逻辑
  }
  
  // PC端原有逻辑：仅当检测到永久关闭Cookie时才隐藏弹窗
  // 修复原代码Cookie名称拼写错误（bonusForeverClo11se → bonusForeverClose）
  if (getCookie('bonusForeverClose') === '1') {
    modal.style.display = 'none';
  } else {
    // 强制显示弹窗（确保每次打开都显示）
    modal.style.display = 'flex';
    modal.style.opacity = '1';
  }
});

// 优化按钮点击体验（防止重复点击）
const btnGet = document.querySelector('.btn-get');
const btnLogin = document.querySelector('.btn-login');

// 仅PC端绑定按钮点击事件
if (!isMobile()) {
  btnGet.addEventListener('click', function() {
    this.style.pointerEvents = 'none';
    this.style.opacity = '0.8';
    setTimeout(() => {
      this.style.pointerEvents = 'auto';
      this.style.opacity = '1';
    }, 1000);
  });

  btnLogin.addEventListener('click', function() {
    this.style.pointerEvents = 'none';
    this.style.opacity = '0.8';
    setTimeout(() => {
      this.style.pointerEvents = 'auto';
      this.style.opacity = '1';
    }, 1000);
  });
}
</script>






<!-- 轻量版资源重试脚本（无性能损耗） -->
<script>
  // 极简配置（仅保留核心重试，无多余校验）
  const retryConfig = {
    maxRetry: 3,        // 最多重试3次
    retryDelay: 500     // 重试间隔缩短为500ms（更快响应）
  };
  const retryMap = new Map(); // 仅记录重试次数，无多余逻辑

  // 核心重试函数（极简版）
  function retryResource(el, type) {
    const src = el.href || el.src;
    if (!src || retryMap.get(src) >= retryConfig.maxRetry) {
      retryMap.delete(src);
      return; // 无地址/超次数直接退出
    }
    retryMap.set(src, (retryMap.get(src) || 0) + 1);
    
    // 仅必要日志（减少控制台开销）
    console.warn(`[重试] ${type}: ${src} (${retryMap.get(src)}/${retryConfig.maxRetry})`);
    
    // 延迟重试（仅替换地址，不复制多余属性）
    setTimeout(() => {
      const newSrc = src.includes('?') ? `${src}&_t=${Date.now()}` : `${src}?_t=${Date.now()}`;
      if (type === 'css') el.href = newSrc; // 直接改地址，不重建标签（性能提升）
      else el.src = newSrc;
    }, retryConfig.retryDelay);
  }

  // 初始化（仅绑定错误事件，无遍历/校验）
  window.addEventListener('DOMContentLoaded', () => {
    // 仅绑定错误事件，无多余操作
    document.querySelectorAll('link[rel="stylesheet"]').forEach(link => {
      link.onerror = () => retryResource(link, 'css');
    });
    document.querySelectorAll('script[src]').forEach(script => {
      script.onerror = () => retryResource(script, 'js');
    });
  });
</script>







